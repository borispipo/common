{
  "name": "@fto-consult/common",
  "version": "1.1.7",
  "description": "Un ensemble de bibliothèques et d'utilistaires communs pour le développement d'applications javascript",
  "main": "index.js",
  "scripts": {
    "publish": "npm publish --access=public",
    "unpublish": "npm -f unpublish @fto-consult/common",
    "delete-docs": "node ./deleteDocsFolder",
    "build-docs": "npm run delete-docs && npx jsdoc -r src  -c ./jsdoc.conf.json -d docs",
    "docs": "npm run build-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borispipo/common.git"
  },
  "keywords": [
    "Bibliothèques",
    "utilitaires",
    "libraries",
    "utiles"
  ],
  "author": "Boris Fouomene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/borispipo/common/issues"
  },
  "homepage": "https://github.com/borispipo/common#readme",
  "dependencies": {
    "@craftzdog/pouchdb-adapter-websql-core": "^7.2.3",
    "circular-dependency-plugin": "^5.2.2",
    "color": "^4.2.3",
    "invariant": "^2.2.4",
    "js-base64": "^3.7.2",
    "pouchdb": "^7.3.0",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-find": "^7.3.0",
    "prop-types": "^15.8.1",
    "qs": "^6.11.0",
    "react-is": "^18.2.0",
    "react-native-get-random-values": "^1.8.0",
    "stable-hash": "^0.0.2",
